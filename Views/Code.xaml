<Window x:Class="ColdShineSoft.HttpClientPerformer.Views.Code"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:viewModels="clr-namespace:ColdShineSoft.HttpClientPerformer.ViewModels;assembly=ColdShineSoft.HttpClientPerformer.ViewModels"
		xmlns:widgets="clr-namespace:ColdShineSoft.HttpClientPerformer.Widgets;assembly=ColdShineSoft.HttpClientPerformer.Widgets"
		mc:Ignorable="d" d:DataContext="{d:DesignInstance Type=viewModels:Code,IsDesignTimeCreatable=True}"
		Title="Code" Height="450" Width="800">
	<widgets:TextEditor Text="{Binding Template.Code}" Type="{Binding Template.Type}" />
</Window>